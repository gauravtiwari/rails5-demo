<div class="message" data-id="<%= message.id %>" data-user-id="<%= message.user_id %>">
  <a href="" class="message_profile-pic">
    <i class="material-icons circle red"><%= message.user.short_name %></i>
  </a>
  <a href="" class="message_username"><%= message.user.name %></a>
  <span class="message_timestamp"><%= message.created_at %></span>
  <span class="message_star"></span>
  <span class="message_content"><%= message.text %></span>
</div>
